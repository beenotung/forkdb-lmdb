{
  "name": "forkdb-lmdb",
  "version": "0.1.2",
  "description": "Object-prototype-like abstraction over lmdb.",
  "keywords": [
    "forkdb",
    "lmdb",
    "object prototype",
    "extend",
    "fork"
  ],
  "author": "Beeno Tung <aabbcc1241@yahoo.com.hk> (https://beeno-tung.surge.sh)",
  "license": "BSD-2-Clause",
  "main": "dist/forkdb.js",
  "types": "dist/forkdb.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register **/*.spec.ts",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "postformat": "tslint -p . --fix",
    "prebuild": "npm run clean",
    "build": "tsc",
    "pretest": "npm run format && npm run build && mkdir -p data",
    "prepublishOnly": "npm run test"
  },
  "husky": {
    "hook": {
      "pre-commit": "npm run format"
    }
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "typestub-node-lmdb": "^0.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/mathjs": "^6.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "*",
    "chai": "^4.2.0",
    "husky": "^3.0.9",
    "mocha": "^6.1.4",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/forkdb-lmdb.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/forkdb-lmdb/issues"
  },
  "homepage": "https://github.com/beenotung/forkdb-lmdb#readme"
}
